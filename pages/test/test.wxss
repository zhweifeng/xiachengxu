.imgWrap {
  position: absolute;
  top: 200px;
  right: 0;
  text-align: center;
  width: 100%;
  background-color: #fff;
}

.slideLeftTopOut {
  transition: all .5s ease-in;
  top: -1000px !important;
  left: -1000px !important;
}

.slideRightTopOut {
  transition: all .5s ease-in;
  top: -1000px !important;
  left: 1000px !important;
}

.slideRightBottomOut {
  transition: all .5s ease-in;
  top: 1000px !important;
  left: 1000px !important;
}

.slideLeftBottomOut {
  transition: all .5s ease-in;
  left: -1000px !important;
  top: 1000px !important;
}

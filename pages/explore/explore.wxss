	animation-delay: .3s;
	-webkit-animation-delay: .3s;
}
@-webkit-keyframes z-opcity {
	from { opacity: 0; -moz-opacity:0;-webkit-opacity:0;-webkit-transform: scale(.7);}
}
@keyframes z-opcity {
	from { opacity: 0; -moz-opacity:0;-webkit-opacity:0;-webkit-transform: scale(.7); transform: scale(.7);}
}
.zToTop{
	-webkit-animation: moveFromBottom 1.5s ease both;

/*圆 -webkit-animation: moveIconUp ease 1.5s both infinite;
    animation: moveToBottom ease 1.5s both infinite;=---------------*/
.pt-page-moveIconUp {
   -webkit-animation: moveToBottom 1.5s both ease ;
    animation: moveToBottom 1.5s both ease;
}
@-webkit-keyframes moveToBottom {
	from {
    -webkit-transform: translateX(0) scale(.5); 
    transform: translateX(0) scale(.5);  
  }
	to {  
    -webkit-transform: translateX(-50rpx)  scale(1); 
    transform: translateX(-50rpx)  scale(1);  

  }
}
@-webkit-keyframes moveIconUp {
	0%{ -webkit-transform: translateY(100%); opacity:0;}
	50%{ -webkit-transform: translateY(0%); opacity:1;}
	100%{ -webkit-transform: translateY(-100%); opacity:0;}
}
@keyframes moveIconUp {
	0% { -webkit-transform: translateY(100%); transform: translateY(100%); opacity:0;}
	50% { -webkit-transform: translateY(0%); transform: translateY(0%); opacity:1;}
	100% { -webkit-transform: translateY(-100%); transform: translateY(-100%); opacity:0;}
}

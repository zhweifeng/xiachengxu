<template name='activity'>
  <view class='popup'>
    <view class='pop-content'>
      <image catchtap='enterActivity' src='/images/icon/redpack.png' mode='scaleToFill'></image>
      <view class='close-btn'>
        <image catchtap='closeActivity' src='/images/icon/close.png' mode='aspectFill'></image>
      </view>
    </view>
  </view>
</template>